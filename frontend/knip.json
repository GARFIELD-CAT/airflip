{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/main.tsx",
    "src/vite-env.d.ts"
  ],
  "project": [
    "src/**/*.{ts,tsx,js,jsx}",
    "!src/**/*.test.{ts,tsx,js,jsx}",
    "!src/**/*.spec.{ts,tsx,js,jsx}"
  ],
  "ignore": [
    "src/vite-env.d.ts",
    "**/*.d.ts"
  ],
  "ignoreDependencies": [
    "@types/*"
  ],
  "vite": {
    "config": ["vite.config.ts"]
  }
} 